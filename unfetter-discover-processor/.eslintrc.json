{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      // ignore the dangle rule, mongo used _id
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "no-console": 0,
      "consistent-return": 0,
      "max-len": 0,
      "no-restricted-syntax": 0,
      "no-var": 0
    },
    "env": {
      "jasmine": true
    }
}