{
  "name": "unfetter-ctf-ingest",
  "version": "0.3.2",
  "description": "ingest cyber threat framework data into unfetter",
  "scripts": {
    "checkup": "npm run clean && npm run build && npm run lint && npm run coverage && npm run test && npm run swagger:verify",
    "test": "jasmine-ts --config=jasmine.json",
    "coverage": "nyc --extension .ts --reporter=lcov --reporter=text-lcov npm test && nyc report",
    "copy:config": "cp -R config dist/config",
    "copy:swagger": "cp src/api/swagger/swagger.yaml dist/api/swagger",
    "build": "npm run clean && tsc -v && mkdir -p ./dist/api/swagger && npm run copy:swagger && tsc -p . --outDir ./dist || exit 0",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist/ tsdocs/ coverage/ .nyc_output/",
    "lint": "tsc -v && tslint src/**/*.ts",
    "ingest:ctf": "ts-node -v && ts-node src/cli/ctf-ingest.cli.ts",
    "ingest:attack-pattern": "ts-node -v && ts-node src/cli/attack-pattern-ingest.cli.ts",
    "docs": "typedoc --out tsdocs --ignoreCompilerErrors",
    "swagger:test": "swagger_swagger_fileName='src/api/swagger/swagger.yaml' swagger project test",
    "swagger:verify": "swagger_swagger_fileName='src/api/swagger/swagger.yaml' swagger project verify",
    "swagger:start": "npm run build && swagger_swagger_fileName='src/api/swagger/swagger.yaml' swagger project start",
    "start": "npm run build && node dist/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "camelcase": "^4.1.0",
    "express": "^4.16.2",
    "jspath": "^0.3.4",
    "moment": "^2.19.2",
    "mongodb": "^2.2.33",
    "node-fetch": "^1.7.3",
    "papaparse": "^4.3.6",
    "swagger-express-mw": "^0.7.0",
    "uuid": "^3.1.0",
    "validator": "^9.1.2",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/istanbul": "^0.4.29",
    "@types/jasmine": "^2.6.0",
    "@types/mongoose": "^4.7.24",
    "@types/node": "^8.0.51",
    "@types/node-fetch": "^1.6.7",
    "@types/papaparse": "^4.1.31",
    "@types/swagger-express-mw": "^0.7.3",
    "@types/uuid": "^3.4.3",
    "@types/validator": "^6.3.0",
    "@types/yargs": "^8.0.2",
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "should": "^13.1.0",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.1"
  }
}
